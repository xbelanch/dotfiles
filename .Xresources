Xft.dpi:    90
Xft.autohint: 0
Xft.antialias:  1
Xft.hinting:    1
Xft.hintstyle:  hintfull

URxvt.termName: rxvt-unicode-256color
URxvt.depth: 32
URxvt.transparent: true
URxvt.shading: 30
URxvt*font: xft:Iosevka Term:pixelsize=18:minspace=false:antialias=true:hinting=true, xft:DejaVu Sans:pixelsize=18
URxvt*boldFont: xft:Iosevka Term:pixelsize=18:minspace=false:antialias=true:hinting=true, xft:DejaVu Sans:pixelsize=18
URxvt*letterSpace: 0
URxvt.scrollBar: false
URxvt*depth: 32
URxvt*foreground: #e4e4ef
URxvt*background: #181818

URxvt.perl-ext-common:  resize-font
URxvt.url-launcher: /usr/bin/xdg-open

*color0:  #2E3436
*color1:  #a40000
*color2:  #4E9A06
*color3:  #C4A000
*color4:  #3465A4
*color5:  #75507B
*color6:  #ce5c00
*color7:  #babdb9
*color8:  #555753
*color9:  #EF2929
*color10: #8AE234
*color11: #FCE94F
*color12: #729FCF
*color13: #AD7FA8
*color14: #fcaf3e
*color15: #EEEEEC

URxvt.keysym.C-minus:  resize-font:smaller
URxvt.keysym.C-equal:  resize-font:bigger
URxvt.keysym.C-0:      resize-font:reset
